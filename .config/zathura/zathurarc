set recolor                 "true"
set continuous-hist-save    "true"
set default-fg              '#ebdbb2'
set default-bg              '#1c1c1c'
set recolor-darkcolor       '#ebdbb2'
set recolor-lightcolor      '#1c1c1c'
set guioptions				''
set scroll-page-aware       "true"
set scroll-full-overlap     0.01
set scroll-step             60
set font                    "Iosevka 11"
map R reload
map r rotate
map K zoom in
map J zoom out
map p print
map e scroll half-up
map u scroll half-up
map d scroll half-down
map i recolor
set statusbar-basename true
set selection-clipboard clipboard

set synctex true
set synctex-editor-command "/home/ashish/.local/bin/backward.py %{line} %{input}"
